<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Fetch</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pretty-checkbox@3.0/dist/pretty-checkbox.min.css"/>
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
    <link rel="icon" type="image/x-icon" href="/public/assets/Fetch.jpg" />
  </head>

  <nav class="main-nav">
    <div class="">
      <div class="" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <img
            src="/assets/Fetch.jpg"
            alt="Logo"
            width="100"
            height="50"
            class=""
          />
          <div class="white-nav-main">
            <a
              href="/"
              class="nav-link white-nav-anchor"
              aria-current="page"
            >Home</a>
            <a href="/signup" class="nav-link white-nav-anchor">Signup</a>

            {{#if loggedIn}}
              <a
                href=""
                class="nav-link white-nav-anchor"
                id="logout"
              >Logout</a>
            {{else}}
              <a href="/login" class="nav-link white-nav-anchor">Login</a>
            {{/if}}

            {{#if loggedIn}}
              <script src="/js/logout.js"></script>
            {{/if}}
            <a
              href="/leaderboard"
              class="nav-link white-nav-anchor"
            >Leaderboard</a>
            <a href="/favorites" class="nav-link white-nav-anchor">Favorites</a>
          </div>
          <a href="/vote">
            <button
              type="button"
              class="btn btn-dark btn-lg fs-3 nav-vote-button"
              id="vote-button"
            >Start Rating Dogs!</button>
          </a>

        </div>
      </div>
    </div>
  </nav>
  <div>
  </div>

  {{{body}}}
  <script src="/js/main.js"></script>
</html>